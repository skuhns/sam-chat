{
  "version": 8,
  "glyphs": "https://demotiles.maplibre.org/font/{fontstack}/{range}.pbf",
  "sources": {
    "osm": {
      "type": "vector",
      "url": "pmtiles://https://inyanufi7jqi7in0.public.blob.vercel-storage.com/map_tiles/chicago/osm.pmtiles",
      "minzoom": 0,
      "maxzoom": 14
    },
    "contours": {
      "type": "vector",
      "url": "pmtiles://https://inyanufi7jqi7in0.public.blob.vercel-storage.com/map_tiles/chicago/chicago_contour.pmtiles",
      "minzoom": 0,
      "maxzoom": 14
    }
  },
  "layers": [
    { "id": "bg", "type": "background", "paint": { "background-color": "#0f0e0e" } },
    {
      "id": "water",
      "type": "fill",
      "source": "osm",
      "source-layer": "water",
      "paint": { "fill-color": "#468189" }
    },
    {
      "id": "transportation",
      "type": "line",
      "source": "osm",
      "source-layer": "transportation",
      "paint": { "line-color": "#868189" }
    }
  ],
  "extra": {
    "id": "contour-lines",
    "type": "line",
    "source": "contours",
    "source-layer": "contour",
    "paint": {
      "line-color": "#b9b1a7",
      "line-opacity": 1
    }
  }
}
